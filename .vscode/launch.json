{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug AstDump",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/CPPProject",
            "args": [
                "${workspaceFolder}/TestVectors/Test.cpp"
            ],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
